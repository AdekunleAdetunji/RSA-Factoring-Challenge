#!/usr/bin/python3

import math
def factorize(n):
    if n % 2 == 0:
        print("{}={}*{}".format(n, 2, n // 2))
        return

    for factor in range(3, math.isqrt(n) + 1, 2):
        if n % factor == 0:
            print("{}={}*{}".format(n, factor, n // factor))
            break;

factorize(4)
factorize(12)
factorize(34)
factorize(128)
factorize(1024)
factorize(4958)
factorize(1718944270642558716715)
factorize(9)
factorize(99)
factorize(999)
factorize(9999)
factorize(9797973)
factorize(49)
factorize(239809320265259)
